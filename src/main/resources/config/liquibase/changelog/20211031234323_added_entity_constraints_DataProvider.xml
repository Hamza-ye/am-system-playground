<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity DataProvider.
    -->
    <changeSet id="20211031234323-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="data_provider"
                                 constraintName="fk_data_provider__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>

        <addForeignKeyConstraint baseColumnNames="last_updated_by_id"
                                 baseTableName="data_provider"
                                 constraintName="fk_data_provider__last_updated_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>

        <addForeignKeyConstraint baseColumnNames="family_id"
                                 baseTableName="data_provider"
                                 constraintName="fk_data_provider__family_id"
                                 referencedColumnNames="id"
                                 referencedTableName="family"/>
    </changeSet>
</databaseChangeLog>
