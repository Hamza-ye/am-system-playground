<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Team.
    -->
    <changeSet id="20211031234318-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="team"
                                 constraintName="fk_team__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>

        <addForeignKeyConstraint baseColumnNames="last_updated_by_id"
                                 baseTableName="team"
                                 constraintName="fk_team__last_updated_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>

        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="team"
                                 constraintName="fk_team__person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="rel_team__assigned_to_warehouse"
                                 constraintName="fk_rel_team__assigned_to_warehouse__team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="assigned_to_warehouse_id"
                                 baseTableName="rel_team__assigned_to_warehouse"
                                 constraintName="fk_rel_team__assigned_to_warehouse__assigned_to_warehouse_id"
                                 referencedColumnNames="id"
                                 referencedTableName="warehouse"/>
    </changeSet>
</databaseChangeLog>
